{{#each this}}
  <li class='notes__line'>
    <ul class='notes__line__list'>
      <li class='notes__line__column'><div class='notes__line__column__wrapper'>{{{image}}}
        </div></li>
      <li class='notes__line__column column-one'>
        {{name}}</li>
      <li class='notes__line__column column-two'>{{created}}</li>
      <li class='notes__line__column column-three'>{{category}}</li>
      <li class='notes__line__column column-four'>{{content}}</li>
      <li class='notes__line__column column-five'>{{dates}}</li>
      <li class='notes__line__column column-six'>
        <button type='button' class='notes__line__button-edit' data-index={{id}}>
          <i class='fa-solid fa-pencil notes__line__button-edit' data-index={{id}}></i></button>
        <button type='button' class='notes__line__button-archived' data-index={{id}}>
          <i class='fa-solid fa-box-archive'></i></button>
        <button type='button' class='notes__line__button-deleted' data-index={{id}}>
          <i class='fa-solid fa-trash-can'></i></button>
      </li>
    </ul>

  </li>
{{/each}}